apiVersion: networking.istio.io/v1alpha3
kind: Gateway
metadata:
  name: kubeflow-gateway
spec:
  selector:
    istio: ${component.istio.ingressGateway}
  servers:
  - port:
      number: 80
      name: http
      protocol: HTTP
    hosts:
    - '${component.kubeflow.name}.${dns.domain}'
    # - '*'

apiVersion: v1
kind: Service
metadata:
  name: dex
  namespace: ${component.dex.namespace}
  labels:
    component: dex
spec:
  selector:
    component: dex
  ports:
  - name: worker
    protocol: TCP
    port: 5556
---
apiVersion: v1
kind: Service
metadata:
  name: dex-api
  namespace: ${component.dex.namespace}
spec:
  selector:
    component: dex
  ports:
  - name: api
    protocol: TCP
    port: 5557

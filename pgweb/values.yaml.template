image: "sosedoff/pgweb:0.9.7"

databaseUrl:      postgres://${component.postgresql.user}:${component.postgresql.password}@${component.postgresql.url}:${component.postgresql.port}/${component.postgresql.database}?sslmode=[mode]

ingress:
  enabled: true
  fqdn: ${component.pgweb.name}.${component.ingress.fqdn}
  path: /
  imageTag: v0.1.1
  imagePullPolicy: IfNotPresent
service:
  type: ClusterIP
  port: 8081

resource "aws_s3_bucket" "main" {
  bucket = local.bucket
  acl = var.acl
  force_destroy = true
  # versioning is still a resource block
  versioning {
    enabled = var.versioning
  }
  # tags is now a map (not resource block)
  tags = {
    "Name" = var.name
  }
  lifecycle {
    prevent_destroy = false
  }
}

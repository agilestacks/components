variable "client_aws_access_key" {
  type = "string"
  default = ""
}

variable "client_aws_secret_key" {
  type = "string"
  default = ""
}

provider "aws" {
  alias  = "client"

  access_key = "${var.client_aws_access_key}"
  secret_key = "${var.client_aws_secret_key}"
  region         = "${var.client_aws_region}"
}
